  "contentVersion": "1.0.0.0",
  "parameters": {
    "ApiUri": {
      "type": "string",
      "minLength": 1
    }
  },
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.CustomProviders/resourceProviders",
      "name": "hr",
