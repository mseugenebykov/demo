      "location": "eastus",
      "properties": {
        "resourceTypes": [
          {
            "name": "employees",
            "routingType": "proxy",
            "endpoint": "[concat(parameters('ApiUri'), 'Employees')]"
          }
        ]
      }
    }
  ],
  "outputs": {}
}
